
<% include ../include/header.ejs %>
<!-- Site wrapper -->
<div class="wrapper">

  <% include ../include/menu.ejs %>



  <!-- =============================================== -->

   <% include ../include/left_sidebar.ejs %>


  <!-- =============================================== -->

  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <h1>
        Retailer Requests
        <small></small>
      </h1>
      <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
        <li class="active">Retailer Requests</li>
      </ol>
    </section>

    <!-- Main content -->
    <section class="content">

    <!--
      <div class="callout callout-info">
       
        

      </div>

      -->


      <!-- Default box -->
      <div class="box">
        <div class="box-header with-border">
          <h3 class="box-title"></h3>
        
          <div class="box-tools pull-right">
            <button type="button" class="btn btn-box-tool" data-widget="collapse" data-toggle="tooltip" title="Collapse">
              <i class="fa fa-minus"></i></button>
            <button type="button" class="btn btn-box-tool" data-widget="remove" data-toggle="tooltip" title="Remove">
              <i class="fa fa-times"></i></button>
          </div>
        </div>
        <div class="box-body">
      

           <section class="content">
            <div class="row">
              <div class="col-xs-12">
                
                <!-- /.box -->

                <div class="box">
                  <!--
                  <div class="box-header">
                    <h3 class="box-title"></h3>
                  </div>
                -->
                  <!-- /.box-header -->
                  <div class="box-body">
                    <table id="example1" class="table table-bordered table-striped">
                      <thead>
                      <tr style="background-color:#3c8dbc;">
                        <th>Sr.No.</th>
                        <th>Name</th>
                        <th>Mobile No</th>
                        <th>Address</th>
                        <th>Accept Request</th>
                        <th>Reject Request</th>
                      </tr>
                      </thead>
                      <tbody>



                       <% for(var i=0; i < users.length; i++) { %>
                        <tr>
                          <td><%= i+1 %></td>
                          <td><%= users[i].firstName %> <%= users[i].lastName %></td>
                          <td><%= users[i].phoneNumber %></td>
                          <td><%= users[i].shopAddress %></td>
                          <td><a href="/userRequestUpdate?id=<%= users[i].id %>&status=1"><button type="button" name="accept" id="accept" class="btn btn-block btn-primary btn-sm">Accept</button></td>

                           <td><a href="/userRequestUpdate?id=<%= users[i].id %>&status=2"><button type="button" name="accept" id="accept" class="btn btn-block btn-danger btn-sm">Reject</button></td>
                           
                        </tr>
                          
                        <% } %>
                       
                        
                      </tbody>
                     
                    </table>
                  </div>
                  <!-- /.box-body -->
                </div>
                <!-- /.box -->
              </div>
              <!-- /.col -->
            </div>
      <!-- /.row -->
    </section>
        </div>
        <!-- /.box-body -->
        <div class="box-footer">
          Footer
        </div>
        <!-- /.box-footer-->
      </div>
      <!-- /.box -->
    </section>
    <!-- /.content -->
  </div>
  <!-- /.content-wrapper -->



